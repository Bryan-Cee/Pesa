{
  "expo": {
    "name": "Pesa",
    "slug": "pesa",
    "version": "1.3.9",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "pesa",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1F4E79"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pesa.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1F4E79"
      },
      "package": "com.pesa.app",
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      "expo-system-ui",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#1F4E79"
        }
      ],
      [
        "expo-share-intent",
        {
          "iosActivationRules": {
            "NSExtensionActivationSupportsText": true,
            "NSExtensionActivationSupportsWebURLWithMaxCount": 0,
            "NSExtensionActivationSupportsImageWithMaxCount": 0,
            "NSExtensionActivationSupportsMovieWithMaxCount": 0
          },
          "androidIntentFilters": [
            "text/plain"
          ],
          "iosShareExtensionName": "Pesa Share"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b05e8d9b-a42c-4554-8025-5bb2d98d2f5b"
      }
    },
    "owner": "bryce_7"
  }
}
